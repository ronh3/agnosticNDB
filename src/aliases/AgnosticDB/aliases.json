[
       {
              "name": "adb help",
              "isActive": "yes",
              "isFolder": "no",
              "command": "",
              "regex": "^adb$",
              "script": "if agnosticdb and agnosticdb.ui then agnosticdb.ui.show_help() end",
              "matchall": "no",
              "isLua": "yes"
       },
       {
              "name": "adb politics",
              "isActive": "yes",
              "isFolder": "no",
              "command": "",
              "regex": "^adb\\s+politics$",
              "script": "if agnosticdb and agnosticdb.ui then agnosticdb.ui.show_politics() end",
              "matchall": "no",
              "isLua": "yes"
       },
       {
              "name": "adb highlights toggle",
              "isActive": "yes",
              "isFolder": "no",
              "command": "",
              "regex": "^adb\\s+highlights\\s+(on|off)$",
              "script": "if agnosticdb and agnosticdb.highlights then agnosticdb.highlights.toggle(matches[2] == 'on') end",
              "matchall": "no",
              "isLua": "yes"
       },
       {
              "name": "adb highlights reload",
              "isActive": "yes",
              "isFolder": "no",
              "command": "",
              "regex": "^adb\\s+highlights\\s+reload$",
